apiVersion: 1

providers:
- name: 'VictoriaMetricsDashboards'
  orgId: 1
  folder: "Node Exporter"
  type: file
  disableDeletion: true
  editable: false
  updateIntervalSeconds: 240
  allowUiUpdates: false
  options:
    path: "{{ grafana_path_dashboards }}"

# https://grafana.com/docs/administration/provisioning/#dashboards
